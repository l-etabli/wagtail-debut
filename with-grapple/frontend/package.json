{
    "name": "frontend_nextjs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "export": "next export",
        "storybook": "start-storybook -p 3001 -s ./public",
        "build-storybook": "npx build-storybook -s public",
        "fixcode": "npx prettier \"**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
        "new": "node cli/bin.js new",
        "new:container": "node cli/bin.js new --container",
        "test": "jest --watch",
        "test:ci": "jest -w 1 --ci"
    },
    "dependencies": {
        "@sentry/browser": "^5.29.2",
        "@sentry/integrations": "^5.29.2",
        "@sentry/node": "^5.29.2",
        "@sentry/webpack-plugin": "^1.14.0",
        "@svgr/webpack": "^5.5.0",
        "@zeit/next-source-maps": "0.0.4-canary.1",
        "@apollo/client": "3.1.1",
        "deepmerge": "^4.2.2",
        "lodash": "4.17.20",
        "graphql": "^15.3.0",
        "i18next": "^19.8.4",
        "next": "11.1.1",
        "next-compose-plugins": "^2.2.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "@types/react": "^17.0.0",
        "classnames": "^2.2.6",
        "prop-types": "^15.7.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern 'frontend/**' && jest --onlyChanged",
            "pre-push": ""
        }
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@storybook/addon-a11y": "^6.1.11",
        "@storybook/addon-actions": "^6.1.11",
        "@storybook/addon-links": "^6.1.11",
        "@storybook/addon-viewport": "^6.1.11",
        "@storybook/addons": "^6.1.11",
        "@storybook/react": "^6.1.11",
        "@testing-library/jest-dom": "^5.11.8",
        "@testing-library/react": "^11.2.2",
        "@testing-library/user-event": "^12.6.0",
        "babel-loader": "^8.2.2",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "husky": "^4.3.6",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "jest-svg-transformer": "^1.0.0",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "react-test-renderer": "^17.0.1",
        "style-loader": "^2.0.0",
        "typescript": "^4.1.3"
    }
}
